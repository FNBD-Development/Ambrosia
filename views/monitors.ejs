            href="/monitors"
          >
            <span class="material-symbols-rounded"> monitor_heart </span>
            Monitors
          </a>

          <li>
            <a
              class="flex items-center gap-x-3.5 py-2 px-2.5 text-sm text-slate-700 rounded-md hover:bg-gray-100 dark:hover:bg-zinc-900/80 dark:text-slate-400 dark:hover:text-slate-300"
              href="javascript:;"
            >
              <span class="material-symbols-rounded"> import_contacts </span>
              Documentation
            </a>
          </li>
        </ul>
      </nav>
    </div>
    <!-- End Sidebar -->

    <!-- Content -->
    <div class="w-full pt-10 px-4 sm:px-6 md:px-8 lg:pl-72">
      <!-- Page Heading -->
      <header class="" id="header">
        <div class="max-w-[85rem] px-4 sm:px-3 lg:px-8 lg:py-3 mx-auto">
          <div
            class="grid sm:grid-cols-1 md:sm-cols-2 lg:grid-cols-3 xl:grid-cols-3 gap-4"
          >
            <div
              class="flex flex-col bg-white border shadow-sm rounded-md dark:bg-zinc-800/50 dark:border-zinc-800 p-4"
            >
              <div class="flex justify-between items-center">
                <h1 class="text-2xl font-semibold dark:text-white">Google</h1>
                <perc class="text-2xl text-gray-800 dark:text-gray-200"
                  >100%</perc
                >
              </div>
              <div class="mt-2">
                <div
                  class="dark:bg-zinc-700/50 flex items-center gap-1 dark:border-zinc-900 p-2 rounded-md"
                >
                  <div class="bg-green-500 rounded-full h-7 w-3"></div>
                  <div class="bg-green-500 rounded-full h-7 w-3"></div>
                  <div class="bg-green-500 rounded-full h-7 w-3"></div>
                  <div class="bg-green-500 rounded-full h-7 w-3"></div>
